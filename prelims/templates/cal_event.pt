<div id="event_${event.id}">
  <div class="panel panel-primary">
    <div class="panel-heading clickable"
      data-toggle="collapse"
      data-parent="#event_${event.id}"
      data-target="#event_${event.id}_body"
      >
      <h2>${event.name}</h2>
    </div>
    <div id="event_${event.id}_body" class="panel-collapse collapse in">
      <div class="panel-body">
        <div id="event_${event.id}_panel_body">
          <div id="event_${event.id}_cal" style="float: left;">
            ${structure: event_cal}
          </div>
          <div id="event_${event.id}_prelims" style="float: right;">
            <div class="panel panel-info">
              <div class="panel-heading">
                Currently scheduled prelims
              </div>
              <div class="panel-body">
                ${structure: prelims}
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>

