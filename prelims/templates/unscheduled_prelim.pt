<div id="event_${event.id}_unscheduled_${student}">
  <div class="row">
    <div tal:condition="deleteable" class="col-lg-1">
      <form role="form" class="form-horizontal delete_unscheduled_prelim_form"
        action="/delete_unscheduled_prelim" method="post"
        >
        <input type="hidden" name="prelim_id" value="${prelim.id}">
        <input type="hidden" name="event_id" value="${event.id}">
        <button type="submit" class="btn btn-danger">Delete</button>
      </form>
    </div>
    <label class="col-lg-1"><strong>${student}</strong></label>
    <div tal:repeat="f fac" tal:content="f" class="col-lg-1"></div>
    <div tal:content="f_alt" class="col-lg-1" style="color: grey;"></div>
    <div tal:condition="not:deleteable" class="col-lg-1"></div>
    <div class=" col-lg-5">
      <!--
      <em><p style="overflow: hidden; text-overflow: ellipsis; white-space: nowrap;" tal:content="prelim.title" /></em>
      -->
      <a href="#" class="prelim_title" id="prelim_${prelim.id}_title"
        data-type="text" data-pk="${prelim.id}" data-url="/update_prelim_title" data-emptytext="No Title"
        tal:content="prelim.title"
        ></a>
    </div>
    <div class="col-lg-1">
      <a tal:condition="paper_url" tal:attributes="href paper_url">paper</a>
      <p tal:condition="not:paper_url">(no pdf)</p>
    </div>
  </div>
</div>
